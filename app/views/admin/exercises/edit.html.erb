<h1>Редактировать упражнение</h1>

<% form_for (@exercise, :url => {:controller => "admin/exercises", :action => "update" }, :html => { :multipart => true}, :method => :post ) do |f| %>
	<%= render :partial => "form", :locals => {:f => f} %>
	<%= f.submit "Сохранить" %>
<% end %>

<% content_for :sub_menu do %>
	<%= render :partial => "admin/menus/admin_main_submenu" %>
<% end %>
